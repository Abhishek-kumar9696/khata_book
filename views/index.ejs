 <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>File List</title>
</head>
<body>
    <% files.forEach(function(file) { %>
        <h3><%= file %></h3>
        <a href="/edit/<%= file %>">edit</a>
    <% }) %>
</body>
</html>
